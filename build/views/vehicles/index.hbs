<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="/">Dashboard</a></li>
        <li class="breadcrumb-item active" aria-current="page"><a href="/Vehicles">Vehicles</a></li>
    </ol>
</nav>


<div class="form-row">
    <div class="form-group col-md-2">
        <a type="button" class="btn btn-primary btn-block mb-2 ml-3" href="/Vehicles/Create">Create</a>
    </div>
</div>

<div class="form-group">
    <div class="col-sm-12 col-md-12 col-lg-12">
        <div class="table-responsive">
            <table class="table table-bordered table-sm table-striped border-bottom border-left border-right">
                <thead class="thead-light">
                    <tr>
                        <th scope="col" width="150px" style="min-width: 150px">Plate</th>
                        <th scope="col" width="150px" style="min-width: 150px">Type</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each vehicles}}
                        <tr>
                            <td>{{plate}}</td>
                            <td>{{type}}</td>
                        </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>